    <script>
        const API = 'http://localhost:1117/api';
        let isRecording = false;
        let recognition = null;
        
        // Walking Cursor
        const cursor = document.getElementById('walkingCursor');
        let cursorX = 0, cursorY = 0;
        let targetX = 0, targetY = 0;
        
        document.addEventListener('mousemove', (e) => {
            targetX = e.clientX;
            targetY = e.clientY;
        });
        
        function animateCursor() {
            cursorX += (targetX - cursorX) * 0.1;
            cursorY += (targetY - cursorY) * 0.1;
            cursor.style.left = cursorX + 'px';
            cursor.style.top = cursorY + 'px';
            
            // Flip based on direction
            if (targetX > cursorX + 5) cursor.style.transform = 'scaleX(1)';
            else if (targetX < cursorX - 5) cursor.style.transform = 'scaleX(-1)';
            
            requestAnimationFrame(animateCursor);
        }
        animateCursor();
        
        // Time
        function updateTime() {
            document.getElementById('currentTime').textContent = new Date().toLocaleTimeString();
        }
        setInterval(updateTime, 1000);
        updateTime();
        
        // Speech Recognition
        if ('webkitSpeechRecognition' in window || 'SpeechRecognition' in window) {
            const SR = window.SpeechRecognition || window.webkitSpeechRecognition;
            recognition = new SR();
            recognition.continuous = true;
            recognition.interimResults = true;
            recognition.lang = 'en-US';
            
            recognition.onresult = async (e) => {
                let final = '', interim = '';
                for (let i = e.resultIndex; i < e.results.length; i++) {
                    if (e.results[i].isFinal) final += e.results[i][0].transcript;
                    else interim += e.results[i][0].transcript;
                }
                
                document.getElementById('transcript').textContent = interim || final || 'Listening...';
                
                if (final) {
                    await sendVoice(final);
                }
            };
            
            recognition.onerror = (e) => { if (e.error !== 'no-speech') stopRecording(); };
            recognition.onend = () => { if (isRecording) recognition.start(); };
        }
        
        function toggleRecording() {
            isRecording ? stopRecording() : startRecording();
        }
        
        function startRecording() {
            if (!recognition) return alert('Use Chrome for voice');
            recognition.start();
            isRecording = true;
            document.getElementById('micBtn').classList.add('recording');
            document.getElementById('micBtn').textContent = 'üî¥';
            document.getElementById('transcript').textContent = 'Listening...';
        }
        
        function stopRecording() {
            if (recognition) recognition.stop();
            isRecording = false;
            document.getElementById('micBtn').classList.remove('recording');
            document.getElementById('micBtn').textContent = 'üé§';
        }
        
        async function sendVoice(text) {
            try {
                const res = await fetch(API + '/ledger/log', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        user_type: document.getElementById('userType').value,
                        raw_transcript: text,
                        page_route: '/dashboard',
                        interaction_mode: 'voice'
                    })
                });
                const data = await res.json();
                
                if (data.analysis) {
                    const a = data.analysis;
                    document.getElementById('badges').innerHTML = `
                        <span class="badge ${a.sentiment}">${a.sentiment}</span>
                        <span class="badge">${a.emotion}</span>
                        <span class="badge">${a.intent}</span>
                        <span class="badge">Empathy: ${Math.round((a.empathyScore || 0.5) * 100)}%</span>
                    `;
                }
                
                if (data.tts_response?.text) {
                    speak(data.tts_response.text);
                }
                
                loadAll();
            } catch (e) {
                console.error('Voice error:', e);
            }
        }
        
        function speak(text) {
            if (window.speechSynthesis) {
                window.speechSynthesis.cancel();
                const u = new SpeechSynthesisUtterance(text);
                u.rate = 1.0;
                window.speechSynthesis.speak(u);
            }
        }
        
        // Table icons mapping
        const tableIcons = {
            'learning_stats': 'üìà', 'ai_memories': 'üß†', 'voice_patterns': 'üé§',
            'knowledge_base': 'üìö', 'crawled_voice_patterns': 'üåê', 'crawled_sources': 'üîç',
            'tts_responses': 'üîä', 'communication_platforms': 'üí¨', 'system_rules': '‚öôÔ∏è',
            'empathy_learning': 'üíö', 'agent_personas': 'üë§', 'voice_command_learning': 'üó£Ô∏è',
            'onboarding_prompts': 'üìù', 'empathy_patterns': 'üíõ', 'self_explanation': 'üìñ',
            'countries': 'üåç', 'global_interaction_ledger': 'üìä', 'boss_voice_inputs': 'üëî',
            'learned_patterns': 'üß¨', 'navigation_stack': 'üß≠', 'frontend_sessions': 'üë•',
            'realtime_feed': 'üì°', 'audit_trail': 'üìú', 'email_queue': 'üìß',
            'crawl_jobs': 'üï∑Ô∏è', 'pdf_processing_jobs': 'üìÑ', 'extracted_products': 'üì¶',
            'embedding_queue': 'üî¢', 'self_coding_requests': 'üíª', 'known_fixes': 'üîß',
            'confirmation_requests': '‚úÖ', 'crawl_queue': 'üìã', 'email_templates': 'üì®'
        };
        
        async function loadDashboard() {
            try {
                const res = await fetch(API + '/dashboard');
                const data = await res.json();
                const s = data.stats || {};
                
                document.getElementById('ledgerCount').textContent = formatNumber(s.ledger_entries || s.total_interactions || 0);
                document.getElementById('bossCount').textContent = formatNumber(s.boss_inputs || 0);
                document.getElementById('teamCount').textContent = formatNumber(s.team_inputs || 0);
                document.getElementById('patternsCount').textContent = formatNumber(s.learned_patterns || 0);
                document.getElementById('crawledCount').textContent = formatNumber(s.web_crawls || s.web_pages_crawled || 0, true);
                document.getElementById('productsCount').textContent = formatNumber(s.products_cataloged || 0);
                document.getElementById('ttsCount').textContent = formatNumber(s.tts_responses || s.tts_responses_given || 0);
                document.getElementById('sessionsCount').textContent = formatNumber(s.active_sessions || 0);
                document.getElementById('auditCount').textContent = formatNumber(s.audit_entries || 0);
                document.getElementById('accuracyScore').textContent = (s.accuracy_score || 85).toFixed(1) + '%';
                
                // Tables
                if (data.tables) {
                    document.getElementById('tablesGrid').innerHTML = data.tables.map(t => `
                        <div class="table-item" onclick="showTable('${t.name}')">
                            <div class="table-icon">${tableIcons[t.name] || 'üìã'}</div>
                            <div class="table-name">${t.name.replace(/_/g, ' ')}</div>
                            <div class="table-count">${t.count} rows</div>
                        </div>
                    `).join('');
                }
            } catch (e) { console.error('Dashboard error:', e); }
        }
        
        async function loadFeed() {
            try {
                const res = await fetch(API + '/feed');
                const feed = await res.json();
                document.getElementById('feedList').innerHTML = (feed.length ? feed : [
                    { icon: 'üì°', title: 'System ready', feed_meta: 'Waiting for interactions', created_at: new Date() }
                ]).slice(0, 15).map(f => `
                    <div class="feed-item">
                        <span class="feed-icon">${f.icon || 'üìå'}</span>
                        <div class="feed-content">
                            <div class="feed-title">${f.title || 'Event'}</div>
                            <div class="feed-meta">${f.description || ''} ‚Ä¢ ${formatTime(f.created_at)}</div>
                        </div>
                    </div>
                `).join('');
            } catch (e) {}
        }
        
        async function loadCrawledSites() {
            try {
                const res = await fetch(API + '/crawled-sites');
                const sites = await res.json();
                document.getElementById('crawlList').innerHTML = sites.slice(0, 20).map(s => `
                    <div class="crawl-item">
                        <div class="crawl-domain">${s.domain}</div>
                        <div class="crawl-pages">${formatNumber(s.pages_crawled || 0)} pages ‚Ä¢ ${formatNumber(s.patterns_extracted || 0)} patterns</div>
                    </div>
                `).join('') || '<div class="crawl-item">Loading sites...</div>';
            } catch (e) {}
        }
        
        async function loadPatterns() {
            try {
                const res = await fetch(API + '/patterns');
                const patterns = await res.json();
                document.getElementById('patternList').innerHTML = patterns.slice(0, 10).map(p => `
                    <div class="pattern-item">
                        <div style="color: var(--vv-gold)">${(p.pattern_name || 'Pattern').substring(0, 35)}</div>
                        <div style="font-size: 0.7em; color: var(--vv-text-dim)">${p.occurrence_count || 0}x ‚Ä¢ ${p.pattern_type || 'unknown'}</div>
                    </div>
                `).join('') || '<div class="pattern-item">Learning patterns...</div>';
            } catch (e) {}
        }
        
        async function loadVoiceCommands() {
            try {
                const res = await fetch(API + '/voice-commands');
                const commands = await res.json();
                document.getElementById('voiceList').innerHTML = commands.slice(0, 10).map(c => `
                    <div class="command-item">
                        <div class="command-text">${c.command_text || c.raw_text || 'Command'}</div>
                        <div class="command-type">${c.command_type || c.intent || ''} ‚Ä¢ ${c.times_used || 0}x</div>
                    </div>
                `).join('') || '<div class="command-item">Listening for commands...</div>';
            } catch (e) {}
        }
        
        async function loadEmpathy() {
            try {
                const res = await fetch(API + '/empathy-patterns');
                const empathy = await res.json();
                document.getElementById('empathyList').innerHTML = empathy.slice(0, 8).map(e => `
                    <div class="pattern-item">
                        <div style="color: #4CAF50">${e.emotion_detected || 'Emotion'}</div>
                        <div style="font-size: 0.7em; color: var(--vv-text-dim)">${(e.effectiveness_score * 100).toFixed(0)}% effective ‚Ä¢ ${e.times_used}x</div>
                    </div>
                `).join('') || '<div class="pattern-item">Learning empathy...</div>';
            } catch (e) {}
        }
        
        function formatNumber(n, useK = false) {
            if (useK && n >= 1000) return (n / 1000).toFixed(0) + 'K';
            return n.toLocaleString();
        }
        
        function formatTime(dateStr) {
            if (!dateStr) return '';
            return new Date(dateStr).toLocaleTimeString();
        }
        
        function showTable(name) {
            alert('Table: ' + name + '\n\nClick to view details (feature coming soon)');
        }
        
        function showDetail(type) {
            console.log('Show detail:', type);
        }
        
        function loadAll() {
            loadDashboard();
            loadFeed();
            loadCrawledSites();
            loadPatterns();
            loadVoiceCommands();
            loadEmpathy();
        }
        
        // Initial load
        loadAll();
        
        // Auto-refresh
        setInterval(loadAll, 5000);
    </script>
</body>
</html>
